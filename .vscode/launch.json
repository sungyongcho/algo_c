  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/sol",
      "args": ["<input.txt"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "launchCompleteCommand": "exec-run",
      "linux": {
        "preLaunchTask": "C/C++: g++ build active file",
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb"
      },
    }
  ]
}
